{
	"name": "Nicebots Dark Theme",
	"type": "dark",
	"colors": {
		"editor.background": "#1E1E1E",
		"editor.foreground": "#FFFFF0",

		"editor.selectionBackground": "#333333",
		"editorCursor.foreground": "#cccccc",

		"panel.background": "#1A1A1B",
		"panelTitle.activeForeground": "#DC143C",
		"panelTitle.inactiveForeground": "#e7e7e799",
		"panelTitle.activeBorder": "#DC143C",

		"sideBar.background": "#1A1A1B",
		"sideBarSectionHeader.background": "#2A2B2D",
	
		"statusBar.background": "#2A2B2D",

		"activityBar.background": "#2A2B2D",
		"activityBar.foreground": "#FFEFD5",
		"activityBarBadge.foreground": "#FFFFFF",
		"activityBarBadge.background": "#44475a",
		"sideBarTitle.foreground": "#DC143C",

		"tab.activeBorder": "#DC143C",
		"tab.activeBackground": "#1E1E1E",
		"tab.activeForeground": "#DC143C",
		"tab.inactiveForeground": "#FFFFFF",
		"tab.inactiveBackground": "#2A2B2D"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#4C5866"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Storage Type",
			"scope": [
				"storage.type",
			],
			"settings": {
				"foreground": "#73FCD6",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Storage Modifier",
			"scope": [
				"storage.modifier"
			],
			"settings": {
				"foreground": "#FF2F92"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword.control",
				"storage.type.function.python"
			],
			"settings": {
				"foreground": "#DC143C",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Preprocessor",
			"scope": [
				"keyword.control.directive.conditional",
				"keyword.control.directive.line",
				"keyword.control.directive.define",
				"punctuation.definition.directive",
				"keyword.control.directive.include",
				"meta.preprocessor.include"
			],
			"settings": {
				"foreground": "#DC143C",
			}
		},
		{
			"name": "Macros, Caps",
			"scope": [
				"entity.name.function.preprocessor",
				"constant.other.caps.python"
			],
			"settings": {
				"foreground": "#E34234",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Logical ,Assignment",
			"scope": [
				"keyword.operator.logical",
				"keyword.operator.ternary",
				"punctuation.separator.slice.python",
				"keyword.operator.assignment.python",
				"keyword.operator.assignment.c",
				"keyword.operator.assignment.compound.python",
				"keyword.operator.assignment.compound.c",
				"keyword.operator.assignment.compound.bitwise.c"
			],
			"settings": {
				"foreground": "#CC0033",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Bitwise",
			"scope": [
				"keyword.operator.bitwise",

			],
			"settings": {
				"foreground": "#CC0033",
			}
		},
		{
			"name": "Greater Operator",
			"scope": [
				"punctuation.separator.slice.python",
				"keyword.operator.sizeof",
			],
			"settings": {
				"foreground": "#CC0033",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Lesser Operator",
			"scope": [
				"keyword.operator.comparison",
				"keyword.operator.arithmetic",
				"keyword.operator.increment",
				"keyword.operator.decrement",
				"keyword.operator.c",
			],
			"settings": {
				"foreground": "#ff003f",
			}
		},
		{
			"name": "Access Operator, Placeholder",
			"scope": [
				"meta.item-access.arguments",
